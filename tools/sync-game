#!/usr/bin/env bash
loc="/data/work/unity-envs/Build$1"
echo "-----------------------------------------------------"
echo "............Syncing from Zemo......................"
echo "-----------------------------------------------------"
rsync -avzhP zemo:${loc}/ ${loc}
echo "-----------------------------------------------------"
machines="ph tbird tball fixer"
for m in $machines
do
  echo "............To $m...................................."
  echo "-----------------------------------------------------"
  rsync -avzhP ${loc}/ ${m}:${loc}
  echo "-----------------------------------------------------"
done
