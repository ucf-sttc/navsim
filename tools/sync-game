#!/usr/bin/env bash
loc="unity-envs/Build$1"
echo "-----------------------------------------------------"
echo "............Syncing from Ultimo......................"
echo "-----------------------------------------------------"
rsync -avzhP ultimo:~/${loc}/ ~/${loc}
echo "-----------------------------------------------------"
echo "............To ph...................................."
echo "-----------------------------------------------------"
rsync -avzhP ~/${loc}/ ph:~/${loc}
echo "-----------------------------------------------------"
echo "............To tbird.............................."
echo "-----------------------------------------------------"
rsync -avzhP ~/${loc}/ tbird:~/${loc}
echo "-----------------------------------------------------"
echo "............To tball................................."
echo "-----------------------------------------------------"
rsync -avzhP ~/${loc}/ tball:~/${loc}

