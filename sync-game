#!/usr/bin/env bash
loc="unity-envs/Build2.8.0"
echo "-----------------------------------------------------"
echo "............Syncing from Ultimo......................"
echo "-----------------------------------------------------"
rsync -avzhP ultimo:~/${loc}/ ~/${loc}
echo "-----------------------------------------------------"
echo "............To ph...................................."
echo "-----------------------------------------------------"
rsync -avzhP ~/${loc}/ ph:~/${loc}
echo "-----------------------------------------------------"
echo "............To librenix.............................."
echo "-----------------------------------------------------"
rsync -avzhP ~/${loc}/ librenix:~/${loc}
echo "-----------------------------------------------------"
echo "............To tball................................."
echo "-----------------------------------------------------"
rsync -avzhP ~/${loc}/ tball:~/${loc}

