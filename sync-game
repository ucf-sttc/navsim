#!/usr/bin/env bash
loc="unity-envs/Build2.6.3"
echo "-----------------------------------------------------"
echo "............Syncing from Zemo........................"
echo "-----------------------------------------------------"
rsync -avzhP zemo:~/${loc}/ ~/${loc}
echo "-----------------------------------------------------"
echo "............To ph...................................."
echo "-----------------------------------------------------"
rsync -avzhP ~/${loc}/ ph:~/${loc}
echo "-----------------------------------------------------"
echo "............To librenix.............................."
echo "-----------------------------------------------------"
rsync -avzhP ~/${loc}/ librenix:~/${loc}
echo "-----------------------------------------------------"
echo "............To tball................................."
echo "-----------------------------------------------------"
rsync -avzhP ~/${loc}/ tball:~/${loc}