#!/usr/bin/env bash

# sync the game from local machine to servers
loc="/data/work/unity-envs/$1"
#echo "-----------------------------------------------------"
#echo "............Syncing from Zemo......................"
#echo "-----------------------------------------------------"
#rsync -avzhP zemo:${loc}/ ${loc}
#echo "-----------------------------------------------------"
machines="zemo"
#machines=""
for m in $machines
do
  echo "............To $m...................................."
  echo "-----------------------------------------------------"
  rsync -avzhP --delete --exclude-from='syncex.txt' ${loc}/ ${m}:${loc}
  echo "-----------------------------------------------------"
done
